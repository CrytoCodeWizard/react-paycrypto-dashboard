    "@mui/lab": "^5.0.0-alpha.147",
    "@mui/material": "^5.14.12",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
